}

div.ttypography .tt {
    margin: 0 4px;
    padding: 2px 4px;
    white-space: nowrap;
    border: 1px solid #CCC;
    background-color: #F8F8F8;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-image: initial;
}

div.ttypography h1,
div.ttypography h2,
div.ttypography h3,
div.ttypography h4,
div.ttypography h5,
